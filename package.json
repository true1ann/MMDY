{
	"scripts": {
		"build-css": "sass scss/styles.scss css/styles.css",
		"watch-css": "sass --watch scss/styles.scss:css/styles.css",
		"build-mincss": "sass --style compressed scss/styles.scss css/styles.css",
		"watch-mincss": "sass --style compressed --watch scss/styles.scss:css/styles.css"
	},
	"dependencies": {
		"sass": "^1.91.0"
	}
}
